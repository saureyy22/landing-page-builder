# Contentful Configuration
# Get these values from your Contentful space settings
CONTENTFUL_SPACE_ID=your_contentful_space_id_here
CONTENTFUL_ACCESS_TOKEN=your_contentful_delivery_api_token_here
CONTENTFUL_PREVIEW_ACCESS_TOKEN=your_contentful_preview_api_token_here
CONTENTFUL_MANAGEMENT_TOKEN=CFPAT-your_contentful_management_token_here
CONTENTFUL_ENVIRONMENT=master

# Next.js Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Contentful App Configuration
CONTENTFUL_APP_ID=your_contentful_app_id_here

# Preview Configuration
REACT_APP_PREVIEW_URL=https://your-landing-pages-deployment.vercel.app

# React App Contentful Configuration (for browser access)
# These should match the server-side values above
REACT_APP_CONTENTFUL_SPACE_ID=your_contentful_space_id_here
REACT_APP_CONTENTFUL_ACCESS_TOKEN=your_contentful_delivery_api_token_here
REACT_APP_CONTENTFUL_PREVIEW_ACCESS_TOKEN=your_contentful_preview_api_token_here
REACT_APP_CONTENTFUL_ENVIRONMENT=master