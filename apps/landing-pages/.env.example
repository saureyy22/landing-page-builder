# Contentful Configuration for Landing Pages
CONTENTFUL_SPACE_ID=your_space_id_here
CONTENTFUL_ACCESS_TOKEN=your_delivery_api_token_here
CONTENTFUL_PREVIEW_ACCESS_TOKEN=your_preview_api_token_here
CONTENTFUL_ENVIRONMENT=master

# Next.js Configuration
NEXT_PUBLIC_SITE_URL=https://your-landing-pages-domain.vercel.app
NEXTAUTH_URL=https://your-landing-pages-domain.vercel.app
NEXTAUTH_SECRET=your_nextauth_secret_here

# Analytics (optional)
NEXT_PUBLIC_GA_ID=your_google_analytics_id_here

# Performance Monitoring
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_vercel_analytics_id_here